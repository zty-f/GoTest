{
  "settings":{
    "index":{
      "refresh_interval":"1s",
      "number_of_shards":"1",
      "number_of_replicas":"2",
      "analysis":{
        "analyzer":{
          "ngram_analyzer":{
            "tokenizer":"ngram_tokenizer"
          }
        },
        "tokenizer":{
          "ngram_tokenizer":{
            "token_chars":[
              "letter",
              "digit",
              "punctuation",
              "symbol"
            ],
            "min_gram":"1",
            "type":"ngram",
            "max_gram":"30"
          }
        }
      }
    }
  }
  "mappings":{
    "_doc":{
      "dynamic":"strict",
      "properties":{
        "id":{
          "type":"keyword"
        },
        "user_id":{
          "type":"long"
        },
        "package_id":{
          "type":"long"
        },
        "msg_str":{
          "type":"text",
          "analyzer":"ngram_analyzer",
          "search_analyzer":"keyword"
        },
        "utm_source":{
          "type":"keyword"
        },
        "pay_type":{
          "type":"keyword"
        },
        "price":{
          "type":"long"
        },
        "source":{
          "type":"long"
        },
        "outer_order_id":{
          "type":"keyword"
        },
        "order_assign":{
          "type":"keyword"
        },
        "leads_assign":{
          "type":"keyword"
        },
        "ct":{
          "type":"date"
        },
        "ut":{
          "type":"date"
        }
      }
    }
  },
  "aliases":{
    "readcamp_sale_order":{}
  }
}